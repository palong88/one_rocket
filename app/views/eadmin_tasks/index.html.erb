


 <div class="row">
      
      <div class="col s6" style="float: left;"> <h5> Your Onboarding Process</h5>
           
      </div>

      <div class="col s6">

      <div style="float: right; margin-top:10px;"> <%= link_to 'Create a New Task', new_admin_task_path, class: "btn btn-success btn-lg" %> 
      </div>

    
    </div>
  </div>


  
 
    <div class="col s12">
      <ul class="tabs" id="tabs">
        <li class="tab col s3" ><a class="active" href="#tabs-1">Learning</a></li>
        <li class="tab col s3"><a  href="#tabs-2">Setting Up</a></li>
        <li class="tab col s3"><a href="#tabs-3">Milestones</a></li>
        <li class="tab col s3"><a href="#tabs-4">Personal Survey</a></li>
      </ul>
