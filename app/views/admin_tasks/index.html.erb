  <script>
    
$(function() {    
    $('.tabs').on('click', function() {
        $('.tabs').removeClass('activeTab'); /* solution for a. */
        $(this).addClass('activeTab'); /* solution for a. */
        var i = $(this).attr('href');
        $('.items p').hide();
        $(i).show();
    });
});
  </script>


 









