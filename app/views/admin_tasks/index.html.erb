


 <div class="row">
      
      <div class="col s6" style="float: left;"> <h5> Your Onboarding Process</h5>
           
      </div>

      <div class="col s6">

      <div style="float: right; margin-top:10px;"> <%= link_to 'Create a New Task', new_admin_task_path, class: "btn btn-success btn-lg" %> 
      </div>

    
    </div>
  </div>


  
 
    <div class="col s12">
      <ul class="tabs" id="tabs">
        <li class="tab col s3" ><a class="active" href="#tabs-1">Learning</a></li>
        <li class="tab col s3"><a  href="#tabs-2">Setting Up</a></li>
        <li class="tab col s3"><a href="#tabs-3">Milestones</a></li>
        <li class="tab col s3"><a href="#tabs-4">Personal Details</a></li>
      </ul>
    </div>
<div class="items">
    <div id="tabs-1" class="col s6 ">
      <h5>Before Start Date</h5>
      <!-- First one Ruby Code-->
      <ul class="collapsible" data-collapsible="accordion">

         <% @admin_tasks.order( 'admin_tasks.due_date DESC' ).each do |admin_task| %>

            <% if admin_task.category == "1" && admin_task.when == "1" %>


                   
                      <li>
                        <div class="collapsible-header ">
                        <div style="float: left;"> 
                              <%= admin_task.title %>  
                          </div>

                          

                          <div style="float: right;">
                            
                             <%= link_to '<i class="material-icons right" >delete</i>'.html_safe, admin_task, method: :delete, data: { confirm: 'Are you sure?' } %>
                                                                            
                               <%= link_to ' <i class="material-icons right">mode_edit</i>'.html_safe, edit_admin_task_path(admin_task) %>

                                 
                                                      


                          </div>
                        </div>

                        <div class="collapsible-body"><p><strong>Description:</strong><%= admin_task.description %></p><br>
                           <p><strong>Media: </strong><a href="http://<%= admin_task.media %> "><%= admin_task.media %> </a></p><br>
                          <p><strong>When: </strong><%= admin_task.due_date %> Day <%= admin_task.when %> Start.</p>






                        </div>
                      </li>
                      
                    
              <%end%>





          <%end%>
           </ul>
