                                                    <div class="details">
                                                      <h5><%= admin_task.title %></h5><br/>
                                                         <muted> <%= admin_task.description.first(40) %>...</muted>
                                                          <p><a href="http://<%= admin_task.media %> "><%= admin_task.media %> </a></p>

                                                      <p><%= admin_task.due_date %> days <%= admin_task.when %> start.</p>
                                                    </div>
                                                    <div class="pull-right hidden-phone">
                                                                              <%= link_to '<button class="btn btn-info btn-xs">
